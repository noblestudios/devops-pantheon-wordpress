{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "ns/event-details",
  "title": "Event Details with Image Slider and Map",
  "description": "Displays event info, image slider, map, add-to-calendar links",
  "category": "widgets",
  "keywords": [],
  "styles": [],
  "supports": {
    "multiple": false,
    "anchor": true,
    "align": false
  },
  "attributes": {
    "anchor": {
      "type": "string",
      "default": ""
    },
    "detailsHeadline": {
      "type": "string",
      "default": "Details"
    },
    "detailsHeadlineTag": {
      "type": "string",
      "default": "h2"
    }
  },
  "example": {
    "viewportWidth": 1440
  },
  "style": [
    "ns-eventDetails",
    "ns-detailsImageSlider"
  ],
  "script": "ns-stakeholderSlider-js",
  "editorScript": "file:./index.js",
  "render": "file:./template-event-details.php"
}
