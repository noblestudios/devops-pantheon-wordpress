{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "ns/article-detail-2",
  "title": "Article Detail v2",
  "description": "Block for blog article content display",
  "category": "custom-blocks",
  "icon": "share",
  "keywords": [
    "blog",
    "template",
    "details",
    "content"
  ],
  "styles": [],
  "supports": {
    "multiple": true,
    "anchor": true,
    "alignText": false,
    "align": false,
    "spacing": {},
    "typography": {},
    "fullHeight": false
  },
  "attributes": {
    "anchor": {
      "type": "string",
      "default": ""
    },
    "showSocials": {
      "type": "boolean",
      "default": true
    },
    "showRelated": {
      "type": "boolean",
      "default": true
    },
    "showCategories": {
      "type": "boolean",
      "default": true
    },
    "socialHeadline": {
      "type": "string",
      "default": "Share This Article"
    },
    "relatedHeadline": {
      "type": "string",
      "default": "Related Articles"
    },
    "categoriesHeadline": {
      "type": "string",
      "default": "Explore Also"
    }
  },
  "example": {
    "viewportWidth": 1440
  },
  "editorScript": "file:./index.js",
  "render": "file:./template-article-detail-2.php",
  "style": [
    "ns-articleDetail-2",
    "ns-articleDetail-sidebar"
  ]
}
